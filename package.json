{
  "name": "functions",
  "scripts": {
    "build": "tsup",
    "lint": "npm run eslint:ts && npm run prettier-format",
    "eslint:ts:fix": "eslint --ext .ts src/ --fix",
    "eslint:ts": "eslint --ext .ts src/",
    "prettier-format": "prettier --check .",
    "prettier-format:fix": "prettier --write .",
    "cleanup": "npm run eslint:ts:fix && npm run prettier-format:fix",
    "test": "uvu -r tsx test '.*/.*\\.test\\.ts'",
    "test:ts": "tsc --noEmit"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@eslint/compat": "^1.3.2",
    "tsup": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.2",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "prettier": "^3.5.2",
    "quicktype-core": "^23.1.3",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "uvu": "^0.5.6"
  },
  "private": true
}
